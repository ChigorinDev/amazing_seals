<div class="row">
    <% data.seals.map(seal => {  %>
      <div class="col s4" id=<%= seal.id %></div>
          <div class="card">
            <div class="card-image">
              <img src=<%= seal.image %>>
              <span class="card-title"><%= seal.name %></span>
            </div>
            <div class="card-content">
              <p><%= seal.about %></p>
            </div>
            <div class="card-action">
              <a class="blue-text" href="#">Узнать больше</a>
            </div>
          </div>
        </div> 
    <% }) %>  
  </div>